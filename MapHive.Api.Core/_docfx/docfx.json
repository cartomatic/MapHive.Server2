{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.sln"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ],
          "src": "../.."
        }
      ],
      "dest": "obj/api",
      "filter": "filter.yml",
      "disableGitFeatures": true,
      "properties": {
        "TargetFramework": "netcoreapp3.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/**.md",
          "api/**/toc.yml",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "api/images/**",
          "articles/images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "bin/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../wwwroot/apidocs",
    "template": [
      "default"
    ]
  }
}